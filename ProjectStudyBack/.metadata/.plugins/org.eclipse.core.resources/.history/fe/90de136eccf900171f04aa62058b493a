package com.yonghyun.projectStudy.service;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;
import org.springframework.transaction.annotation.Transactional;

import com.yonghyun.projectStudy.dao.UsersDao;
import com.yonghyun.projectStudy.domain.Users;

@Service
@Transactional
public class EmployeeServiceImpl implements UsersService {
	@Autowired
	UsersDao employeeDao;

	@Override
	public Users getEmployee(int id) {
		// TODO Auto-generated method stub
		return employeeDao.getEmployee(id);
	}
	
}
